{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/testimonials",
      "destination": "/reviews",
      "permanent": true
    },
    {
      "source": "/demo",
      "destination": "/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/rate-your-experience",
      "has": [
        {
          "type": "host",
          "value": "partners.webbersaurus.com"
        }
      ],
      "destination": "/rate-your-experience.html"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "partners.webbersaurus.com"
        }
      ],
      "destination": "/rate-your-experience.html"
    }
  ]
}
